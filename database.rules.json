{
    "rules": {
        "prototype": {
            "$prototype": {
                "instance": {
                    "$instance": {
                        ".read": true,
                        ".write": true
                    }
                },
                "userInstance": {
                    "$user": {
                    ".read": "$user == auth.uid",
                    ".write": "$user == auth.uid"
                  }
              }
            }
        },
        ".read": false,
        ".write": false  
    }
  }

  
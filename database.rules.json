{
    "rules": {
        "prototype": {
            "$prototype": {
                "instance": {
                    "$instance": {
                        ".read": true,
                        ".write": true
                    }
                },
                "userInstance": {
                    "$user": {
                    ".read": "$user == auth.uid",
                    ".write": "$user == auth.uid"
                  }
              }
            }
        },
        "userInstance": {
            "$user": {
              ".read": "$user == auth.uid",
            ".write": "$user == auth.uid",
              "$prototype": {
                                  "$instance": {
                          ".write": true
                }                
            }
        },
        ".read": false,
        ".write": false  
    }
  }
}